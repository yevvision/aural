<IfModule mod_rewrite.c>
  RewriteEngine On

  # vorhandene Dateien/Ordner: direkt ausliefern
  RewriteCond %{REQUEST_FILENAME} -f [OR]
  RewriteCond %{REQUEST_FILENAME} -d
  RewriteRule ^ - [L]

  # alles andere auf index.html
  RewriteRule ^.*$ /index.html [L]
</IfModule>
